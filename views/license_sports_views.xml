<odoo>

  <record id="license_sports_view_tree" model="ir.ui.view">
    <field name="name">license.sports.view.tree</field>
    <field name="model">license.sports</field>
    <field name="arch" type="xml">
        <tree name="Licenses">
          <field name="season_id"/>
          </tree> 
    </field>
</record>

<record id="license_sports_view_form" model="ir.ui.view">
  <field name="name">license.sports.view.form</field>
  <field name="model">license.sports</field>
  <field name="arch" type="xml">
      <form name="Licenses">
        <field name="season_id"/>
        <field name="athlete_id"/>
        <field name="partner_id"/>
        <field name="athlete_category"/>
        <field name="price"/>
        <button name="action_sell_license" type="object" string="Sell License"></button>           
        </form> 
  </field>
</record>




</odoo>
